es.selectedProducts,\r\n        placeOrderStatus:state.sales.placeOrderStatus,\r\n        currentUser:state.login.currentUser\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps,mapDispatchToProps)(NewSale)\r\n\r\n","C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\sales\\sales.actions.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\assets\\titles.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\customButton\\customButton.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\spinner\\spinner.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\subSection\\subSection.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\assets\\sectionslist.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\newSaleProds\\newSaleProds.component.jsx",["218","219"],"import React, { useState } from 'react'\r\nimport NSProdList from \"../newSaleProdList/newSaleProdList.component\"\r\nimport sideBtn from \"./sideBtn.png\"\r\nimport dwnBtn from \"../../assets/dwnBtn.png\"\r\nimport \"./newSaleProds.styles.css\"\r\n\r\nconst NewSaleProd = ({prod}) =>\r\n{\r\n    const [hide,setHide] = useState(true)\r\n\r\n    return(\r\n        <div className=\"newSaleProd\">\r\n            <div className=\"newSaleProd-Info\">\r\n                <div className=\"nsProd-name\" onClick={()=>setHide(hide=>!hide)} style={{display:\"flex\"}}>\r\n                {!hide ? <img src={sideBtn} alt=\"\" height=\"20px\"/> : <img src={dwnBtn} alt=\"\" height=\"20px\"/>}\r\n                \r\n                <div style={{marginLeft:\"10px\"}}>\r\n                    {prod.telcoName}\r\n                </div>\r\n                </div>\r\n                <img className=\"nsProd-logo\" src={\"http://localhost/StaffApp/logos/\" + `${prod.logoName}`}/>\r\n            </div>\r\n            {   hide ? \r\n                prod.categories.map((category,index)=>\r\n                {\r\n                    return <div className=\"newSale-Prod-Info\" key={index}>\r\n                                <div className=\"newSaleProd-Category\">{category.categoryName}</div>\r\n                                <div className=\"nsProdsList\">\r\n                                    <div className=\"nsProdsHeader\">\r\n                                        <div className=\"newSale-prodName\">Product</div>\r\n                                        <div className=\"newSale-price\">Price</div>\r\n                                        <div className=\"newSale-qty\">Qty</div>\r\n                                        <div className=\"newSale-total\">Total</div>\r\n                                    </div>\r\n                                    <div style={{height:\"auto\",maxHeight:\"200px\",overflow:\"scroll\"}}>\r\n                                    {\r\n                                        category.products.map((prods,index)=>\r\n                                        {\r\n                                            return <NSProdList prods={prods} key={index}/>\r\n                                        })\r\n                                    }\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                }) : null\r\n            }\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default NewSaleProd","C:\\Users\\umers\\Desktop\\evdclientapp\\src\\utils\\fetching.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\sales\\sales.types.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\form-input\\form-input.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\saleVouchers\\saleVouchers.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\salesList\\salesList.component.jsx",["220"],"import React,{useEffect, useState} from \"react\"\r\nimport CustomButton from \"../customButton/customButton.component\"\r\nimport SalesPopUp from \"../salesPopUp/salesPopUp.component\"\r\nimport {fetchListDetailsStart,setCurrentOrder,getDownloadFileLink,cancelSale} from \"../../redux/sales/sales.actions\"\r\nimport { connect } from \"react-redux\"\r\nimport { withRouter } from \"react-router-dom\"\r\nimport \"./salesList.styles.css\"\r\n\r\nconst SalesListComp = ({sale,fetchListDetailsStart,OrderDetails,match,setCurrentOrder,getDownloadFileLink,cancelSale}) =>\r\n{\r\n    const [SalesPopUpVal,setSalesPopUpVal] = useState(false)\r\n    \r\n\r\n    // useEffect(()=>\r\n    // {\r\n    //     if (OrderDetails)\r\n    //     {\r\n    //         //setSalesPopUpVal(true)\r\n    //         console.log(OrderDetails)\r\n    //     }\r\n    // },[OrderDetails])\r\n\r\n    const closePopUp = () =>\r\n    {\r\n        setSalesPopUpVal(false)\r\n    }\r\n\r\n    const openPopUp = async () =>\r\n    {\r\n        fetchListDetailsStart(sale.orderId)\r\n        setSalesPopUpVal(true)\r\n    }\r\n\r\n    const downloadFile = () =>\r\n    {\r\n        let order = {\r\n            OrderId:sale.orderId\r\n        }\r\n        getDownloadFileLink(order)\r\n    } \r\n    \r\n\r\n    return(\r\n        <div className=\"sales-ListOrders\">\r\n            <div className=\"sales-time sales-width\">{sale.saleTime}</div>\r\n            <div className=\"sales-refrenceNo sales-width\" onClick={openPopUp}>{sale.orderId}</div>\r\n            <div className=\"salesPopUpVal\">\r\n               { SalesPopUpVal ? OrderDetails ? <SalesPopUp close={closePopUp} details={OrderDetails} /> : null : null}\r\n            </div>\r\n            <div className=\"sales-fileDownload sales-width\" onClick={downloadFile}>\r\n                <CustomButton btnText=\"download\" downloaded={sale.downloaded}/>\r\n            </div>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nconst mapDispatchToProps = (dispatch) =>\r\n{\r\n    return{\r\n        fetchListDetailsStart:(orderId)=>dispatch(fetchListDetailsStart(orderId)),\r\n        setCurrentOrder:(orderId)=>dispatch(setCurrentOrder(orderId)),\r\n        getDownloadFileLink:(orderId)=>dispatch(getDownloadFileLink(orderId)),\r\n        cancelSale:(orderId)=>dispatch(cancelSale(orderId))\r\n    }\r\n}\r\n\r\nconst mapStateToProps = state =>\r\n{\r\n    return{\r\n        OrderDetails:state.sales.OrderDetails\r\n    }\r\n}\r\n \r\nexport default withRouter(connect(mapStateToProps,mapDispatchToProps)(SalesListComp))","C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\section\\section.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\subField\\subField.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\popUp\\popUp-actions.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\store.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\section\\sectionAction.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\newSaleProdList\\newSaleProdList.component.jsx",[],["221","222"],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\components\\salesPopUp\\salesPopUp.component.jsx",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\popUp\\popUp.types.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\root-reducer.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\root-sagas.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\section\\sectionTypes.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\login\\login.reducers.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\section\\section-Reducer.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\popUp\\popUp-reducer.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\sales\\sales.reducer.js",[],"C:\\Users\\umers\\Desktop\\evdclientapp\\src\\redux\\sales\\sales.sagas.js",["223","224","225","226","227"],"import {takeLatest,all,put,call} from \"redux-saga/effects\"\r\nimport {SalesActionTypes} from \"./sales.types\"\r\nimport {fetchSaleListSuccess,fetchSaleListError,fetchListDetailsSuccess,fetchListDetailError,f